{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "/srcindex.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "db:push" : " prisma db push  --schema=./src/prisma/schema.prisma",
    "migrate": "npx prisma migrate dev --name init --preview-feature --schema=./src/prisma/schema.prisma",
    "migration:save": "npx prisma migrate save --experimental --schema=./src/prisma/schema.prisma",
    "migration:up": "npx prisma migrate up --experimental --schema=./src/prisma/schema.prisma",
    "migration:down": "npx prisma migrate down --experimental --schema=./src/prisma/schema.prisma",
    "generate": "npx prisma generate"
  },
